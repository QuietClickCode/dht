<?xml version="1.0" encoding="UTF-8"?>
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>自定义权限校验标签</short-name>
	<uri>http://www.zgrmdht.com/permission</uri>
	<tag>
		<name>perm</name>
		<tag-class>com.retailers.auth.tag.CheckPermissionTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>url</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</taglib>
